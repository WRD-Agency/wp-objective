{
	"name": "wrd/wp-objective",
	"version": "1.1.3",
	"description": "A set of classes for building consistent WordPress plugins.",
	"type": "library",
	"license": "MIT",
	"autoload": {
		"psr-4": {
			"Wrd\\WpObjective\\": "src/",
			"Tests\\": "tests/"
		},
		"files": [
			"src/functions.php"
		]
	},
	"authors": [
		{
			"name": "wrd-sodapixel",
			"email": "dev@sodapixel.com"
		}
	],
	"require": {
		"php": "^8.3.0"
	},
	"scripts": {
		"doc-facades": "Wrd\\WpObjective\\Scripts\\Doc_Facades_Script::handle"
	},
	"minimum-stability": "stable",
	"require-dev": {
		"pestphp/pest": "^4.1"
	},
	"config": {
		"allow-plugins": {
			"pestphp/pest-plugin": true
		}
	}
}
